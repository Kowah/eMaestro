import sqlite3

conn = sqlite3.connect('eMaestro.db')

cursor = conn.cursor()
cursor.execute("""
CREATE TABLE IF NOT EXISTS eMaestro(
     _id INTEGER PRIMARY KEY AUTOINCREMENT UNIQUE,
     name TEXT,
     mesure INTEGER,
     rythme INTEGER
)
""")
cursor.execute("""INSERT INTO eMaestro(name, mesure, rythme) VALUES(?, ?, ?)""", ("test",1,2))
conn.commit()

cursor.execute("""SELECT name, rythme FROM eMaestro""")
user1 = cursor.fetchone()
print(user1)

conn.close()
